{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"baseUrl": "/",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"downlevelIteration": true
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"config.ts",
		"server.ts"
	]
	// "include": ["next-env.d.ts", "styled-components.d.ts", "**/*.ts", "**/*.tsx"]
}